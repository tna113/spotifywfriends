{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "react/prop-types": 0,
    "forbid-prop-types": 0,
    "no-duplicate-imports": 2,
    "no-irregular-whitespace": 1,
    "spaced-comment": 0,
    "no-unused-vars": 1
  }
}
